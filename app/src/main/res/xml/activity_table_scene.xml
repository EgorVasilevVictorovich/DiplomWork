<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="7000">
       <KeyFrameSet>

           <KeyPosition
               motion:motionTarget="@+id/linearLayout"
               motion:framePosition="0"
               motion:keyPositionType="pathRelative"
               motion:percentX="0.25"
               motion:percentY="0.32" />
           <KeyPosition
               motion:motionTarget="@+id/linearLayout"
               motion:framePosition="49"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.25"
               motion:percentY="0.32" />
           <KeyPosition
               motion:motionTarget="@+id/linearLayout2"
               motion:framePosition="0"
               motion:keyPositionType="pathRelative"
               motion:percentX="0.25"
               motion:percentY="0.32" />
           <KeyPosition
               motion:motionTarget="@+id/linearLayout2"
               motion:framePosition="49"
               motion:keyPositionType="parentRelative"
               motion:percentX="0.25"
               motion:percentY="0.32" />

       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="204dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="238dp"
            android:id="@+id/imageView5"
            android:layout_marginBottom="312dp"
            motion:layout_constraintBottom_toTopOf="@+id/linearLayout2" />
        <Constraint
            android:layout_marginEnd="48dp"
            android:layout_height="55dp"
            android:layout_marginRight="48dp"
            android:layout_marginTop="44dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="49dp"
            android:id="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="@+id/imageView5" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="52dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:id="@+id/imageView7" />
        <Constraint
            android:id="@+id/imageView7"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_marginStart="32dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="32dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="52dp" />
        <Constraint
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="49dp"
            android:layout_height="55dp"
            android:layout_marginEnd="48dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="48dp"
            android:layout_marginTop="44dp"
            motion:layout_constraintStart_toStartOf="@+id/imageView5" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="204dp"
            motion:layout_editor_absoluteX="86dp"
            android:layout_width="238dp"
            android:id="@+id/imageView5"
            motion:layout_constraintBottom_toTopOf="@+id/linearLayout2"
            motion:layout_constraintTop_toTopOf="parent"
            android:alpha="0"
            motion:layout_editor_absoluteY="193dp" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="276dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="60dp"
            android:alpha="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView7" />
        <Constraint
            android:id="@+id/imageView7"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_marginStart="332dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="332dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0"
            android:layout_marginTop="276dp" />
        <Constraint
            android:layout_marginEnd="324dp"
            android:layout_height="55dp"
            android:layout_marginRight="324dp"
            android:layout_marginTop="472dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:alpha="0"
            android:layout_width="49dp"
            android:id="@+id/imageView6" />
    </ConstraintSet>
</MotionScene>